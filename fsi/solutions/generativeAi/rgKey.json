{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "prefix": {
            "type": "string",
            "defaultValue": ""
        },
        "location": {
            "type": "string"
        },
        "aiCmkKeyName": {
            "type": "string",
            "defaultValue": ""
        }
    },
    "variables": {
        "name-compliant-keyvault": "[concat(parameters('prefix'), '-', parameters('location'), '-kv')]"
    },
    "resources": [],
    "outputs": {
        "keyVersion": {
            "type": "string",
            "value": "[if(not(empty(parameters('aiCmkKeyName'))), last(split(reference(resourceId('Microsoft.KeyVault/vaults/keys', variables('name-compliant-keyvault'), parameters('aiCmkKeyName')),'2021-10-01').keyUriWithVersion, '/')), '')]"
        }
    }
}